<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                                      https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <embeddable name="App\Client\Domain\Entity\Client\ClientAddress">
        <field name="street" type="client_address_street"/>
        <field name="city" type="client_address_city"/>
        <field name="state" type="client_address_state"/>
        <field name="zip" type="client_address_zip"/>
    </embeddable>
</doctrine-mapping>